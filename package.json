        "copy-webpack-plugin": "^4.0.1",
        "cordova-icon": "^1.0.0",
        "cordova-splash": "^1.0.0",
        "cross-env": "^5.0.1",
        "cross-spawn": "^5.0.1",
        "css-loader": "^0.28.0",
        "eslint": "^4.18.1",
