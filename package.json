{
    "name": "arches_mobile",
    "version": "0.2.2",
    "description": "A mobile app to manage Arches resources",
    "author": "Farallon Geographics, Inc.",
    "private": true,
    "scripts": {
        "dev": "webpack-dev-server --inline --progress --config build/webpack.dev.conf.js",
        "start": "npm run dev",
        "build": "node build/build.js",
        "build:watch": "cross-env WEBPACK_WATCH=true node build/build.js",
        "unit": "cross-env BABEL_ENV=test karma start test/unit/karma.conf.js --single-run",
        "e2e": "node test/e2e/runner.js",
        "test": "npm run unit && npm run e2e",
        "lint": "eslint --ext .js,.vue src test/unit/specs test/e2e/specs",
        "restore-ios": "yarn remove mapbox-gl-cordova-offline && cordova platform remove ios && cordova platform add ios && yarn add archesproject/mapbox-gl-cordova-offline#master",
        "restore-android": "yarn remove mapbox-gl-cordova-offline && cordova platform remove android && cordova platform add android && yarn add archesproject/mapbox-gl-cordova-offline#master",
        "icons": "cordova-icon && cordova-splash",
        "cordova-build": "yarn run build && cordova build",
        "cordova-run": "yarn run build && cordova run browser",
        "cordova-run-ios": "yarn run build && cordova run ios",
        "ios": "yarn run cordova-run-ios",
        "ios-mojave": "yarn run build && cordova run ios  --buildFlag='-UseModernBuildSystem=0'",
        "cordova-run-android": "yarn run build && cordova run android",
        "android": "yarn run cordova-run-android"
    },
    "dependencies": {
        "@mapbox/geojson-extent": "^0.3.2",
        "@mapbox/mapbox-gl-draw": "^1.2.0",
        "blob-util": "^2.0.2",
        "cordova-android": "^7.1.4",
        "cordova-browser": "^5.0.3",
        "cordova-ios": "^5.1.1",
        "cordova-plugin-camera": "^4.0.3",
        "cordova-plugin-cleartext": "^1.0.0",
        "cordova-plugin-device": "^2.0.2",
        "cordova-plugin-file": "^6.0.1",
        "cordova-plugin-file-transfer": "^1.7.1",
        "cordova-plugin-geolocation": "^4.0.1",
        "cordova-plugin-network-information": "^2.0.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-sqlite-ext": "^5.0.1",
        "date-fns": "^1.29.0",
        "luxon": "^1.6.2",
        "mapbox-gl-cordova-offline": "archesproject/mapbox-gl-cordova-offline#mapbox_1.12.0_upgrade_fresh",
        "moment": "^2.24.0",
        "onsenui": "~2.10.5",
        "pouchdb": "^7.1.1",
        "pouchdb-adapter-cordova-sqlite": "pouchdb-community/pouchdb-adapter-cordova-sqlite#master",
        "pouchdb-find": "^6.4.3",
        "pouchdb-upsert": "^2.2.0",
        "semver": "^7.3.2",
        "uuid": "^3.2.1",
        "vue": "^2.5.17",
        "vue-datetime": "^1.0.0-beta.8",
        "vue-js-toggle-button": "^1.3.0",
        "vue-onsenui": "~2.6.1",
        "vue-router": "^3.0.1",
        "vue-search-select": "archesproject/vue-search-select#master",
        "vue-touch": "^2.0.0-beta.4",
        "vuex": "^3.0.0",
        "weekstart": "^1.0.0"
    },
    "devDependencies": {
        "autoprefixer": "^7.1.2",
        "babel-core": "^6.22.1",
        "babel-eslint": "^7.1.1",
        "babel-loader": "^7.1.1",
        "babel-plugin-istanbul": "^4.1.1",
